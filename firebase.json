{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/*.php",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "gogolink",
          "region": "europe-west3"
      }
      }
    ]
  }
}
